#!/bin/bash
#
# configuration options for structr-ui
#

RUN_OPTS="-cp lib/*:${artifactId}-${version}-${maven.build.timestamp}.${buildNumber}.jar"
JAVA_OPTS="-Xms1g -Xmx1g -XX:+UseNUMA"
MAIN_CLASS="org.structr.Server"
DISPLAY_NAME="${artifactId}-${version}-${maven.build.timestamp}.${buildNumber}.jar"
LOG_FILE="logs/server.log"
PID_FILE="server.pid"
